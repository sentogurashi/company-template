{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-rational-order",
    "./node_modules/prettier-stylelint/config.js"
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": [true, {
      "ignoreAtRules": ["function", "if", "each", "include", "mixin"]
    }]
  }
}